{
  "name": "git-commit-helper-it",
  "version": "1.0.9",
  "type": "module",
  "description": "Generatore automatico di messaggi di commit con Gemini AI",
  "bin": {
    "gch": "./bin/git-commit-helper.cjs",
    "gch-init": "./bin/init-config.cjs"
  },
  "files": [
    "bin",
    "src",
    "locales"
  ],
  "scripts": {
    "init": "node ./bin/init-config.cjs",
    "setup": "node ./bin/setup.cjs",
    "dev": "node ./src/index.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@google/genai": "^1.8.0",
    "chalk": "^5.4.1",
    "execa": "^7.1.1",
    "figlet": "^1.8.1",
    "inquirer": "^12.7.0",
    "node-fetch": "^3.3.2",
    "os-locale": "^6.0.2"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  },
  "keywords": [
    "git",
    "commit",
    "gemini",
    "ai",
    "automation"
  ],
  "author": {
    "name": "Maurizio Tolomeo",
    "email": "maurizio.tolomeo@outlook.it",
    "url": "https://github.com/moris88"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
